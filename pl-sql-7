CREATE SEQUENCE sequence_1 
start with 1 
increment by 1 
minvalue 0 
maxvalue 100 
cycle; 
 
Sequence in descending order. 
CREATE SEQUENCE sequence_2 
start with 100 
increment by -1 
min value 1 
max value 100 
cycle; 
 
 
CREATE TABLE students(ID number(10),NAME char(20)); 
INSERT into students VALUES(sequence_1.nextval,'Shubham'); 
INSERT into students VALUES(sequence_1.nextval,'Aman'); 
